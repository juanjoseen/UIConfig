# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'
workspace 'UIConfig'
use_frameworks!

def utils_pods
    pod 'lottie-ios'
end

target 'UIConfig' do
  # Pods for UIConfig
  utils_pods
  project 'UIConfigApp.xcodeproj'
end

target 'UIConfigApp' do
  # Pods for UIConfigApp
  utils_pods
  project 'UIConfigApp.xcodeproj'
end
